
<div id="users">

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
 
  <div class="field">
    
    <%= f.label :role%>
    
    <%= f.select :role, ['owner','student']%>
  </div>
  
  
  <div class="actions" >
    <%= f.submit%>
  </div>
</div>